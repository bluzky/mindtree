{
  "name": "@bluzky/mindtree",
  "version": "1.0.2",
  "private": false,
  "description": "Mindmap viewer for web",
  "license": "MIT",
  "author": "Daniel Nguyen <bluesky.1289@gmail.com>",
  "repository": "github:bluzky/mindtree",
  "main": "dist/js/mindtree.js",
  "files": [
    "dist/",
    "src/",
    "index.html",
    "LICENSE",
    "README.md",
    "example"
  ],
  "scripts": {
    "watch": "webpack --watch --progress --mode development",
    "start": "webpack-dev-server",
    "dev": "webpack --mode development",
    "clean": "shx rm -rf dist",
    "build": "npm run clean && cross-env NODE_ENV=\"production\" webpack"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-syntax-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.0",
    "random-graph": "^0.0.3",
    "shx": "^0.3.2",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "non-layered-tidy-tree-layout": "^2.0.2",
    "two.js": "^0.7.0-stable.1"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.scss$",
        "options": {
          "semi": "true"
        }
      }
    ]
  },
  "babel": {
    "plugins": [
      "@babel/syntax-class-properties",
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
